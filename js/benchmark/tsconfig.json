{
  "compilerOptions": {
       "target": "es2022",
       "module": "Node16",
       "moduleResolution": "node16",
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
       "esModuleInterop": true,
       "strict": true,
       "skipLibCheck": true,
    "resolveJsonModule": true

  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
