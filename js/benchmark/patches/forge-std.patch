diff --git a/hardhat.config.js b/hardhat.config.js
new file mode 100644
index 0000000..6aa92ae
--- /dev/null
+++ b/hardhat.config.js
@@ -0,0 +1,31 @@
+/** @type import('hardhat/config').HardhatUserConfig */
+module.exports = {
+  solidity: {
+    version: "0.8.30",
+    settings: {
+      optimizer: {
+        enabled: true,
+        runs: 200
+      },
+    }
+  },
+  paths: {
+    sources: "./test",
+  },
+  solidityTest: {
+    forking: {
+      rpcEndpoints: {
+        mainnet: process.env.ALCHEMY_URL,
+        optimism_sepolia: "https://sepolia.optimism.io/",
+        arbitrum_one_sepolia: "https://sepolia-rollup.arbitrum.io/rpc/",
+        needs_undefined_env_var: "${UNDEFINED_RPC_URL_PLACEHOLDER}"
+      }
+    },
+    fuzz: {
+      // Used to ensure deterministic fuzz execution
+      seed: "0x1234567890123456789012345678901234567890",
+      runs: 256
+    },
+  }
+};
+
