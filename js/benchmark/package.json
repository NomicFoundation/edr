{
  "name": "benchmark",
  "version": "1.0.0",
  "dependencies": {
    "@nomicfoundation/hardhat-utils": "3.0.0-next.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0"
  },
  "devDependencies": {
    "@ethereumjs/util": "^9.1.0",
    "@nomicfoundation/edr": "workspace:*",
    "@nomicfoundation/edr-helpers": "workspace:*",
    "@types/argparse": "^2.0.16",
    "@types/lodash": "^4.14.123",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "argparse": "^2.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.2.1",
    "hardhat": "3.1.10",
    "hardhat2": "npm:hardhat@2.28.4",
    "lodash": "^4.17.11",
    "prettier": "^3.2.5",
    "simple-git": "^3.25.0",
    "smol-toml": "^1.3.0",
    "tsx": "^4.19.3",
    "typescript": "~5.8.2"
  },
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "pnpm run build:dev",
    "build:dev": "tsc --build --incremental .",
    "compareForge": "node --noconcurrent_sweeping --noconcurrent_recompilation --max-old-space-size=28000 --import tsx src/index.ts compare-forge",
    "eslint": "eslint \"src/**/*.ts\"",
    "help": "tsx src/index.ts -h",
    "lint": "pnpm run prettier && pnpm run eslint",
    "lint:fix": "pnpm run prettier --write",
    "precompareForge": "pnpm run preproviderBenchmark",
    "preproviderBenchmark": "cd ../../crates/edr_napi && pnpm build && cd ../../js/helpers && pnpm build",
    "presoltests": "pnpm run preproviderBenchmark",
    "presoltestsBenchmark": "pnpm run preproviderBenchmark",
    "pretest": "cd ../.. && pnpm build:dev",
    "prettier": "prettier --check \"**/*.{js,ts}\"",
    "providerBenchmark": "node --noconcurrent_sweeping --noconcurrent_recompilation --max-old-space-size=28000 --import tsx src/index.ts provider-benchmark",
    "reportForge": "tsx src/index.ts report-forge",
    "reportProviderBenchmark": "tsx src/index.ts report-provider-benchmark",
    "soltests": "node --noconcurrent_sweeping --noconcurrent_recompilation --max-old-space-size=28000 --import tsx src/index.ts solidity-tests",
    "soltestsBenchmark": "node --noconcurrent_sweeping --noconcurrent_recompilation --max-old-space-size=28000 --import tsx src/index.ts solidity-tests-benchmark",
    "test": "node --import tsx/esm --test \"test/*.ts\"",
    "verifyProviderBenchmark": "tsx src/index.ts verify-provider-benchmark"
  },
  "type": "module"
}
